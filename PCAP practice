class ClassA
  var = 1

  def __init__(self, prop):
    prop1 = prop2 = prop
class ClassB(ClassA):
  def __init__(self,prop):
    prop3 = prop*2
    super().__init__(prop)

object = ClassB(2)    ## creating an instance of classB 



